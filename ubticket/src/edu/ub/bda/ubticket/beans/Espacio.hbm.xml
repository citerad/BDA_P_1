<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.ub.bda.ubticket.beans.Espacio" table="ESPACIO">
        <id name="id" column="ID">
            <generator class="identity" />
        </id>
        <property name="nombre" column="NOMBRE" />
        <property name="aforo" column="AFORO" /> 
        <property name="telefono" column="TELEFONO" />
        <property name="direccion" column="DIRECCION" />
        <property name="email" column="EMAIL" />
        <property name="longitud" column="LONGITUD" /> 
        <property name="latitud" column="LATITUD" />
    </class>
</hibernate-mapping>
